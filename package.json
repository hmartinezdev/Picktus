{
  "name": "picktus",
  "version": "0.0.1",
  "description": "Picktus application",
  "main": "index.js",
  "author": "Hugo Martinez",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "@firebase/util": "^0.3.4",
    "@types/react-transition-group": "^2.9.1",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "firebase": "^6.0.2",
    "firebase-admin": "^7.3.0",
    "firebase-functions": "^3.0.1",
    "firebase-functions-test": "^0.1.6",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.0",
    "next": "^8.1.0",
    "polyglot": "^0.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-transition-group": "^4.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-observable": "^1.1.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.2",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "dev": "node --inspect devServer.js",
    "lint": "tslint -c tslint.json 'src/app/**/*.tsx' 'src/app/**/*.ts'",
    "lint:ci": "tslint -t json -o tslint-report.json -c tslint.json 'src/app/**/*.tsx' 'src/app/**/*.ts'",
    "prettier": "prettier --write **/*.ts --write **/*.tsx",
    "test": "jest",
    "test:update": "jest -u",
    "test:ci": "jest --coverage",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "preserve": "yarn run server:copy && yarn run build:public && yarn run build:funcs && yarn run build:app && yarn run deps:copy && yarn run deps:install",
    "serve": "NODE_ENV=production firebase serve --only functions,hosting",
    "deploy:all": "firebase deploy --token \"$FIREBASE_TOKEN\"",
    "build:public": "cpx \"src/public/**/*.*\" \"dist/public\" -C && cpx \"src/app/static/**/*.*\" \"dist/public/static\" -C",
    "build:funcs": "babel \"src/functions\" --out-dir \"dist/functions\"",
    "build:app": "NODE_ENV=production next build \"src/app\"",
    "deps:copy": "cpx \"*{package.json,yarn.lock}\" \"dist/functions\"",
    "server:copy": "cpx \"src/server/**/*.*\" \"src/functions/app/server\"",
    "deps:install": "cd \"dist/functions\" && yarn install"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@firebase/app-types": "^0.4.0",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.2",
    "@types/jest": "^24.0.13",
    "@types/js-cookie": "^2.2.2",
    "@types/next": "^8.0.5",
    "@types/react": "^16.8.17",
    "@types/react-redux": "^7.0.9",
    "@types/redux-mock-store": "^1.0.1",
    "@types/uuid": "^3.4.4",
    "@zeit/next-typescript": "^1.1.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.3",
    "cpx": "^1.5.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "enzyme-to-json": "^3.3.5",
    "firebase-tools": "^6.10.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "react-test-renderer": "16.8.6",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": "8"
  }
}
