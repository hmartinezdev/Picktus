{
  "name": "Picktus",
  "version": "0.0.1",
  "description": "Picktus application",
  "main": "index.js",
  "author": "Hugo Martinez",
  "license": "public",
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^2.0.3",
    "@types/enzyme": "^3.1.13",
    "firebase": "^5.4.0",
    "isomorphic-unfetch": "^2.1.1",
    "next": "^6.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-observable": "^1.0.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.2.2",
    "styled-jsx": "^2.2.7"
  },
  "scripts": {
    "dev": "next \"src/app\"",
    "lint": "tslint -c tslint.json 'src/app/**/*.tsx' 'src/app/**/*.ts'",
    "prettier": "prettier --write **/*.ts --write **/*.tsx",
    "test": "jest",
    "test:ci": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "preserve": "npm run build:public && npm run build:funcs && npm run build:app && npm run deps:copy && npm run deps:install",
    "serve": "NODE_ENV=production firebase serve --only functions,hosting",
    "deploy:all": "firebase deploy --token \"$FIREBASE_TOKEN\"",
    "build:public": "cpx \"src/public/**/*.*\" \"dist/public\" -C && cpx \"src/app/static/**/*.*\" \"dist/public/static\" -C",
    "build:funcs": "babel \"src/functions\" --out-dir \"dist/functions\"",
    "build:app": "NODE_ENV=production next build \"src/app\"",
    "deps:copy": "cpx \"*{package.json,package-lock.json}\" \"dist/functions\"",
    "deps:install": "cd \"dist/functions\" && npm i"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/runtime": "^7.0.0-rc.1",
    "@firebase/app-types": "^0.3.2",
    "@types/jest": "^23.3.1",
    "@types/next": "^6.1.2",
    "@types/react": "^16.4.8",
    "@types/react-redux": "^6.0.6",
    "@zeit/next-typescript": "^1.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "coveralls": "^3.0.2",
    "cpx": "^1.5.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "firebase-admin": "^5.12.1",
    "firebase-functions": "^1.0.4",
    "firebase-tools": "^4.1.1",
    "jest": "^23.4.2",
    "prettier": "^1.14.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.4.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  }
}
