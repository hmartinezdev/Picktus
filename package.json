{
  "name": "picktus",
  "version": "0.0.1",
  "description": "Picktus application",
  "main": "index.js",
  "author": "Hugo Martinez",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@firebase/util": "^0.2.15",
    "@types/enzyme": "^3.1.13",
    "@types/uuid": "^3.4.4",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "firebase": "^5.5.5",
    "firebase-admin": "^6.0.0",
    "firebase-functions": "^2.0.5",
    "isomorphic-unfetch": "^2.1.1",
    "js-cookie": "^2.2.0",
    "next": "^8.1.0",
    "polyglot": "^0.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-transition-group": "^2.4.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-observable": "^1.0.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.2.2",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "dev": "node --inspect devServer.js",
    "lint": "tslint -c tslint.json 'src/app/**/*.tsx' 'src/app/**/*.ts'",
    "lint:ci": "tslint -t json -o tslint-report.json -c tslint.json 'src/app/**/*.tsx' 'src/app/**/*.ts'",
    "prettier": "prettier --write **/*.ts --write **/*.tsx",
    "test": "jest",
    "test:update": "jest -u",
    "test:ci": "jest --coverage",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "preserve": "npm run server:copy && npm run build:public && npm run build:funcs && npm run build:app && npm run deps:copy && npm run deps:install",
    "serve": "NODE_ENV=production firebase serve --only functions,hosting",
    "deploy:all": "firebase deploy --token \"$FIREBASE_TOKEN\"",
    "build:public": "cpx \"src/public/**/*.*\" \"dist/public\" -C && cpx \"src/app/static/**/*.*\" \"dist/public/static\" -C",
    "build:funcs": "babel \"src/functions\" --out-dir \"dist/functions\"",
    "build:app": "NODE_ENV=production next build \"src/app\"",
    "deps:copy": "cpx \"*{package.json,package-lock.json}\" \"dist/functions\"",
    "server:copy": "cpx \"src/server/**/*.*\" \"src/functions/app/server\"",
    "deps:install": "cd \"dist/functions\" && npm i"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@firebase/app-types": "^0.4.0",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.1",
    "@types/js-cookie": "^2.2.0",
    "@types/next": "^6.1.2",
    "@types/react": "^16.4.8",
    "@types/react-redux": "^6.0.6",
    "@types/redux-mock-store": "^1.0.1",
    "@zeit/next-typescript": "^1.1.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.2",
    "cpx": "^1.5.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "firebase-tools": "^4.1.1",
    "jest": "^23.4.2",
    "prettier": "^1.14.0",
    "react-test-renderer": "16.4.2",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  }
}
